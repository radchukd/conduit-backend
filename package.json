{
  "name": "graphql-typescript-starter",
  "version": "1.0.0",
  "author": "Dmytro Radchuk <radchuk.d@gmail.com>",
  "homepage": "https://github.com/radchukd/graphql-typescript-template",
  "description": "Graphql template using typescript and express",
  "license": "MIT",
  "keywords": [
    "node",
    "typescript",
    "express",
    "graphql"
  ],
  "scripts": {
    "build": "tsc",
    "debug": "nodemon --inspect dist/index.js",
    "dev": "nodemon src/index.ts --watch src",
    "generate": "graphql-codegen --config codegen.json",
    "lint": "eslint \"./**/*.ts\"",
    "test": "jest --forceExit",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-rate-limit": "^5.1.1",
    "graphql": "^14.6.0",
    "graphql-depth-limit": "^1.1.0",
    "helmet": "^3.22.0",
    "mongodb": "^3.5.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.3",
    "@graphql-codegen/typescript": "1.13.3",
    "@graphql-codegen/typescript-operations": "1.13.3",
    "@graphql-codegen/typescript-resolvers": "1.13.3",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-rate-limit": "^5.0.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/helmet": "0.0.45",
    "@types/jest": "^25.2.1",
    "@types/mongodb": "^3.5.11",
    "@types/node": "^13.13.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3",
    "npm": "^6.14.4",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
